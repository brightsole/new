{
  "name": "new-cli",
  "version": "0.0.0",
  "description": "new-cli",
  "keywords": [
    "cli",
    "new",
    "brightsole"
  ],
  "main": "index.js",
  "repository": "git@github.com:one19/new-cli.git",
  "author": "Drew Showalter <maelstroswim@gmail.com>",
  "license": "MIT",
  "bin": {
    "new-cli": "bin/index.js"
  },
  "_files": [
    "bin/index.js",
    "dist/index.js",
    "dist/index.map"
  ],
  "scripts": {
    "lint": "eslint --fix .",
    "precommit": "npm run build && lint-staged",
    "build": "parcel build -o dist/index.js -t node index.js && shebang-it",
    "prepush": "ava",
    "test": "ava",
    "coverage": "nyc --reporter=html --reporter=text ava"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@brightsole/eslint-config-lint-node": "^0.0.2",
    "ava": "^0.25.0",
    "husky": "^0.14.2",
    "lint-staged": "^7.2.2",
    "nyc": "^12.0.2",
    "parcel-bundler": "^1.8.1",
    "shebang-it": "^0.0.3"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "lodash": "4.17.10",
    "signale": "^1.2.1"
  }
}
