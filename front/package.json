{
  "name": "new-front",
  "version": "0.0.0",
  "description": "frontend",
  "main": "index.html",
  "scripts": {
    "precommit": "yarn build && lint-staged",
    "start": "parcel index.html",
    "clean": "rm -rf docs/*.js docs/*.html docs/*.map dist coverage .cache .nyc_output",
    "prebuild": "yarn clean",
    "build": "parcel build index.html",
    "postbuild": "mkdir docs || true && cp dist/*.js docs/ && cp dist/*.html docs/",
    "test": "jest",
    "lint": "eslint . --fix",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/one19/new-front.git"
  },
  "keywords": [
    "frontend",
    "react"
  ],
  "author": "Drew Showalter <maelstroswim@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/one19/new-front/issues"
  },
  "homepage": "https://github.com/one19/new-front#readme",
  "devDependencies": {
    "@brightsole/eslint-config-lint-front": "^0.0.1",
    "a-plus-forms": "^0.7.17",
    "a-plus-forms-json-validator": "^1.0.6",
    "axios": "^0.18.0",
    "babel-plugin-auto-import": "^0.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chroma-js": "^1.3.5",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "jsdom": "^12.0.0",
    "lint-staged": "^7.2.2",
    "nock": "^9.1.6",
    "parcel-bundler": "^1.9.7",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.4.5"
  },
  "dependencies": {}
}
